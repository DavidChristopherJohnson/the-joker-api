{
  "name": "the-joker-api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -w -l info -P app.js",
    "logs:db": "docker logs the-joker-api-mongodb_container-1",
    "start:db": "docker compose up -d",
    "stop:db": "docker compose down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidChristopherJohnson/the-joker-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DavidChristopherJohnson/the-joker-api/issues"
  },
  "homepage": "https://github.com/DavidChristopherJohnson/the-joker-api#readme",
  "dependencies": {
    "@fastify/autoload": "^4.0.0",
    "@fastify/mongodb": "^5.0.0",
    "@fastify/sensible": "^4.0.0",
    "fastify": "^3.0.0",
    "fastify-cli": "^3.1.0",
    "fastify-plugin": "^3.0.0",
    "mongodb": "^4.6.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "tap": "^16.1.0"
  }
}
